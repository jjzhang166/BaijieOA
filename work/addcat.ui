	\"bordercolor\"		: me._vo.bordercolor,
	\"textcolor\"	: me._vo.textcolor,
	\"desc\"			: me._vo.desc,
	\"isshare\"			: ( me._vo.isshare == \"on\" ? 1 : 0)

};
(new dao.table(\"t_schedule_cat\")).Add(obj);
me._Successt_schedule_cat_Hint = \"添加成功\";
