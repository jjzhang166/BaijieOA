try
{
	
		
		
		if( \"\" == me._vo.name ) throw \"请输入name\";
		
			if( me._vo.name.Length >128 ) throw \"输入的数据长度超过了name的限制\";
			
			if( me._vo.desc.Length >1024 ) throw \"输入的数据长度超过了desc的限制\";
			

		
}
catch (ex)
{
	me._Errort_role_Hint= string(ex);
	return;
}
var obj=
{
	\"name\"			: me._vo.name,
	\"desc\"			: me._vo.desc,
	\"isdeleted\"			: 0,
	\"roletype\"			: const \"模型.组织.角色.角色类型.群组\",
	\"unitid\"			: 0

};
(new dao.table(\"t_role\")).Add(obj);
var roleid = staticcall(\"dao.table.GetLastInsertID\");
var aUserIds = me._vo.userids.Split(\"|\");
each(a,aUserIds)
{
	if( int(a) > 0 )
	{
		var obj={ \"roleid\":roleid,\"userid\":a};
		(new dao.table(\"t_role_user\")).Add(obj);
	}
}

me._Successt_role_Hint = \"添加成功\";
"
