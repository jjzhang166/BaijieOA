OnGetData:"
var ret = (new dao.table(\"t_wf_job\")).GetData(
	_CurPage,
	_PageSize,
	\"select a.id,a.appname,a.segment,a.appver,a.createtime,a.userid,b.alias,c.starttime
	from t_wf_job a left join t_wf_process c on a.processid=c.id left join t_user b on c.userid=b.id 
	\",
	\"where isnotify=0 and isnull(finishtime) and (a.status=? or a.status=?)  and a.userid=? order by a.createtime desc\",
	const(\"Workflow.Model.Job.未处理\"),
	const(\"Workflow.Model.Job.未读\"),
	me.会话.UserID);
return ret.Datas;
"
OnGetTotalPage:"
var ret = (new dao.table(\"t_wf_job\")).GetDataCount(
	_PageSize,
	\"\",
	\"where isnotify=0 and isnull(finishtime) and (status=? or status=?) and userid=?\",
	const(\"Workflow.Model.Job.未处理\"),
	const(\"Workflow.Model.Job.未读\"),
	me.会话.UserID);

return ret.TotalPage;
"
